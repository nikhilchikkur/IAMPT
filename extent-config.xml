<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
    	<!-- report theme -->
    	<!-- standard, dark -->
    	<theme>dark</theme>
    
		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>
		
		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>
		
        <!-- title of the document -->
        <documentTitle>Test Report</documentTitle>
        
        <!-- report name - displayed at top-nav -->
        <reportName>
        <![CDATA[
            <img src='CMT.PNG'/>
        ]]></reportName>
        
        <!-- report headline - displayed at top-nav, after reportHeadline -->
        <reportHeadline>
        
        </reportHeadline>
        
        <!-- global date format override -->
        <!-- defaults to yyyy-MM-dd -->
        <dateFormat>dd-MMM-yyyy</dateFormat>
        
        <!-- global time format override -->
        <!-- defaults to HH:mm:ss -->
        <timeFormat>hh:mm:ss a</timeFormat>
        
        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
        </scripts>
        
        <!-- custom styles -->
       <styles>
            <![CDATA[
          .menu{width: 100px; height: 10; font-size: 9px;}
  .menu li{list-style: none; float: left; margin-right: 2px; padding: 5px;}
  .menu li:hover, .menu li.active {
        background-color: #f90;
    }
         .report-name { padding-left: 2px; } .report-name > img { float: left;height: 10%;margin-left: 10px;margin-top: 1px;width: 10%; } 
        ]]>
        </styles>
    </configuration>
</extentreports>